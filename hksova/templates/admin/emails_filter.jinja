{%- extends "_main.jinja" -%}
{%- block main -%}

<h2>Generování seznamu emailů</h2>
<table>
<form action="{{url_for('admin' ~ year.year ~ ".generating_emails")}}" method="post">
{{ form.csrf_token }}
{% for choice in form.filter %}
    <tr><td>{{ choice }}</td><td>{{ choice.label }}</td></tr>
{% endfor %}
</table>
<p><p>
    <button type="submit" class="btn btn-primary">Generovat seznam emailů</button>
    <button type="button"  class="btn btn-primary" onclick="history.back()">Zpět</button>
</form>

{%- endblock -%}