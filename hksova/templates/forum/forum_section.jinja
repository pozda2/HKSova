{% extends "_main.jinja" %}
{% block main%}
{% if section %}

	{% for post in section %}
	<table class="table table-dark table-striped">
		<tr><td>{{post.name}}</td><td class="text-right">{{post.insertedAt}}</td></tr>
		<tr><td colspan=2>{{ post.text }} </td></tr>
	</table>
	{% endfor %}
	</table>
	<div class="d-flex justify-content-center">{{ pagination.links }}</div>

{% endif %}
{% endblock %}